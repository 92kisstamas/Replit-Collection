class Main {
  public static void main(String[] args) {
    int allTurbines = 25;
    int turbineCounter = 0;
    int turbinePower = 0;
    int sumPower = 0;

    for (int i = 0; i < allTurbines; i++) {
      if (i < 10) {
        turbineCounter++;
        turbinePower = 2000;
        sumPower += turbinePower;
        System.out.println("A(z) " + turbineCounter + ". számú szélturbina teljes fordulaton működik, " + turbinePower + " MWh-t termelve. A farmon termelt összenergia jelenleg " + sumPower + " MWh.");
      } else if (i < 20) {
        turbineCounter++;
        turbinePower = 1000;
        sumPower += turbinePower;
        System.out.println("A(z) " + turbineCounter + ". számú szélturbina fél fordulaton működik, " + turbinePower + " MWh-t termelve. A farmon termelt összenergia jelenleg " + sumPower + " MWh.");
      } else if (i < 25) {
        turbineCounter++;
        turbinePower = 0;
        sumPower += turbinePower;
        System.out.println("A(z) " + turbineCounter + ". számú szélturbina nem működik, " + turbinePower + " MWh-t termelve. A farmon termelt összenergia jelenleg " + sumPower + " MWh.");
      } else {
        System.out.println("Valami váratlan történt.");
      }
    }
  }
}


class Main {
  public static void main(String[] args) {
    System.out.println("DogCatMouse");
    for (int i = 1; i <= 100; i++) {
      if (i % 15 == 0) {
        System.out.println(i + " #MOUSE");
      } else if (i % 3 == 0) {
        System.out.println(i + " !DOG");
      } else if (i % 5 == 0) {
        System.out.println(i + " ?CAT");
      } else {
        System.out.println(i);
      }
    }
  }
}


class Main {
  public static void main(String[] args) {
    for (int i = 1; i <= 20; i++) {
      if (i % 2 != 0) {
      System.out.println(i + " egy páratlan szám.");
    } else if (i == 10) {
      System.out.println("A 10 a legszebb páros szám.");
    } else {
      System.out.println(i + " egy páros szám.");
    }
    }
  }  
}


class Main {
  public static void main(String[] args) {
    System.out.println("OnlineMagazine");

    int age = 18;

    if (age < 18) {
      System.out.println("Sajnáljuk, de ez a cikk csak 18 éven felüliek számára érhető el.");
    } else if (age == 18) {
      System.out.println("Pont 18? Gratulálunk, üdv a felnőttek között! Jó szórakozást a cikkhez!");
    } else {
      System.out.println("Köszönjük! Elolvashatod ezt a cikket.");
    }
  }
}


class Main {
  public static void main(String[] args) {
    int number1 = 4;
    int number2 = 15;

    if (number1 < number2) {
     System.out.println(number1 + " kisebb, mint " + number2);
     } else {
     System.out.println(number1 + " nagyobb, mint " + number2);
     }
  }
}


class Main {
  public static void main(String[] args) {
    int totalDwarves = 10;
    int workingDwarves = 5;
    int dwarfCounter = 1;

    while (dwarfCounter <= workingDwarves) {
      System.out.println("A(z) " + dwarfCounter + ". törpe a bányában.");
      dwarfCounter++;
    }

    for (; dwarfCounter <= totalDwarves; dwarfCounter++) {
      System.out.println("A(z) " + dwarfCounter + ". törpe még otthon alszik.");
    }
  }
}


class Main {
  public static void main(String[] args) {
    System.out.println("Hello skylark");
    int rabbitPopulation = 15;
    int monthCounter = 1;

    for (; monthCounter <= 6; monthCounter++) {
      rabbitPopulation = rabbitPopulation * 2;
      System.out.println("A(z) " + monthCounter + ". hónapban " + rabbitPopulation + " a nyúlpopuláció.");
    }

    for (; monthCounter <= 12; monthCounter++) {
      rabbitPopulation = rabbitPopulation * 3;
      System.out.println("A(z) " + monthCounter + ". hónapban " + rabbitPopulation + " a nyúlpopuláció.");
    }
  }
}



