    }
  }
}



class Main14 {
  public static void main(String[] args) {
    System.out.println("Hello countdown!");
    int seconds = 10;
    while (seconds >= 0) {
      System.out.println("Final Countdown: " + seconds);
      seconds--;
    }
  }
}


