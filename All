class Main6 {
  public static void main(String[] args) {
    int distance = 700;
    int gasPrice = 480;
    int meanOfConsumption = 7; 
    System.out.println(600 * 6.5 / 100);
    System.out.println(600 * 6.5 / 100 * gasPrice);
    System.out.println(distance * meanOfConsumption / 100 * gasPrice);
  }  
}


