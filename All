class Main4 {
  public static void main(String[] args) {
    int sumOfTwoNumbers = 10 + 10;
    String sumOfTwoStrings = "egy," + " megérett a meggy";
    System.out.println(sumOfTwoNumbers);
    System.out.println(sumOfTwoStrings);
    String myName = "Alex";
    System.out.println("Helló, a nevem " + myName);

    int ultimateAnswer = 1890 / 90 * 2;
    System.out.println("A válasz az életre, a világmindenségre, meg mindenre: " + ultimateAnswer);
  }  
}


