  public static void main(String[] args) {
    printRectangleArea();
    /*System.out.println("A négyszög területe " + calculateRectangleArea(5, 7) + " négyzetméter.");*/
  }  

  public static int calculateRectangleArea(int width, int height) {
  return width * height;
  }

  public static void printRectangleArea() {
  int area = calculateRectangleArea(5, 7);
  System.out.println("A négyszög területe " + area + " négyzetméter.");
  }
}



class Main8 {
  public static void main(String[] args) {
    System.out.println("A felhasználó életkora" + calculateAge(2021, 1992));
  }

  public static int calculateAge ( int currentYear, int birthYear ) {
    int age = currentYear - birthYear;
    return age;
  }
}


