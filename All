class Main12 {
  public static void main(String[] args) {
    int variable = 10;
    localScope();
    System.out.println("the variable in the main method: " + variable);
  }

  public static void localScope() {
      int variable = 20;
      System.out.println("local variable in the function: " + variable);
  }
}


class Main11 {
  public static void main(String[] args) {
    circlesPerimeterArea (15);
  }

  public static double circlesPerimeter (int radiusOfPerimeter) {
    return 2 * 3.14 * radiusOfPerimeter;
  }

  public static double circlesArea (int radiusOfArea) {
    return 3.14 * (radiusOfArea * radiusOfArea);
  }

  public static void circlesPerimeterArea (int radiusFromUser) {
    double perimeter = circlesPerimeter(radiusFromUser);
    double area = circlesArea(radiusFromUser);
    System.out.println("A kör kerülete: " + perimeter + "\nA kör területe: " + area);
  }
}


class Main10 {
  public static void main(String[] args) {
    printRectangleArea(2, 4);
  }  

  public static int calculateRectangleArea(int width, int height) {
    return width * height;
  }

  public static void printRectangleArea(int widthFromUser, int heightFromUser) {
    int area = calculateRectangleArea(widthFromUser, heightFromUser);
    System.out.println("A négyszög területe " + area + " négyzetméter.");
  }
}


